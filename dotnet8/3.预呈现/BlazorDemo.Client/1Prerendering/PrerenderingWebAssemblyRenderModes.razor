@page "/PrerenderingWebAssemblyRenderModes"

<div class="card">
    <div class="card-header">
        预呈现 (WebAssembly)
    </div>
    <div class="card-body">
        <SharedMessage @rendermode="new InteractiveWebAssemblyRenderMode(prerender: true)" Title="你好！Prerender true" />
        <hr />
        <SharedMessage @rendermode="new InteractiveWebAssemblyRenderMode(prerender: false)" Title="你好！Prerender false" />
    </div>
    <div class="card-footer">
        OSDescription:@RuntimeInformation.OSDescription
    </div>
</div>
